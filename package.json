  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/rave-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rave-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "resolutions": {
    "@types/react": "17",
    "@types/react-dom": "17"
  },
