  "license": "MIT",
  "author": "BjoernRave",
  "main": "dist/index.js",
  "module": "dist/rave-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --passWithNoTests",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/rave-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rave-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "resolutions": {
    "@types/react": "17",
    "@types/react-dom": "17"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@hookform/devtools": "^4.3.0",
    "react-dropzone": "11.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@emotion/react": "11.10.5",
