  "license": "MIT",
  "author": "BjoernRave",
  "main": "dist/index.js",
  "module": "dist/rave-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "build-storybook": "build-storybook",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
