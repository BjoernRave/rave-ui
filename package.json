    "@tsconfig/recommended": "^1.0.2",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "babel-loader": "^8.2.5",
    "date-fns": "^2.29.3",
    "dts-cli": "^1.6.3",
    "husky": "^8.0.3",
    "next": "13",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
