{
  "name": "rave-ui",
  "version": "0.1.0",
  "license": "MIT",
  "author": "BjoernRave",
  "main": "dist/index.js",
  "module": "dist/rave-ui.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "build-storybook": "build-storybook",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "storybook": "start-storybook -p 6006",
    "test": "dts test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/rave-ui.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/rave-ui.esm.js",
      "limit": "10 KB"
    }
  ],
  "resolutions": {
    "@types/react": "17",
    "@types/react-dom": "17"
  },
  "dependencies": {
    "@inventhora/json-data": "0.0.2",
    "@inventhora/utils": "0.0.2",
    "react-dropzone": "11.4.2"
  },
  "devDependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@babel/core": "^7.19.6",
    "@emotion/react": "11.10.4",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.6.2",
    "@mui/lab": "5.0.0-alpha.80",
    "@mui/material": "5.6.4",
    "@mui/styled-engine": "5.6.1",
    "@mui/styles": "5.6.1",
    "date-fns": "^2.29.3",
    "@mui/x-date-pickers-pro": "5.0.0-alpha.2",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@storybook/addon-a11y": "^6.5.13",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addons": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@tsconfig/create-react-app": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "babel-loader": "^8.2.5",
    "dts-cli": "^1.6.0",
    "husky": "^8.0.1",
    "next": "12.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-is": "^18.2.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "react-table": "^7.8.0",
    "zod": "^3.19.1"
  }
}
